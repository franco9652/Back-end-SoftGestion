{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": ["error", {
            "parser": "flow",
            "endOfLine": "auto",
            "singleQuote": true
        }],
        "linebreak-style": 0
    }
}
